<aura:component implements="lightning:actionOverride,force:appHostable,lightning:availableForFlowActions,lightning:availableForFlowScreens,flexipage:availableForAllPageTypes" access="global" >
    
    <aura:attribute name="url" type="String"/>
    <aura:attribute name="pageReference" type="Object"/>
    <aura:handler name="init" value="{! this }" action="{! c.init }"/>
    <lightning:navigation aura:id="navService"/>
    <a href="{!v.url}">Link</a>
    
</aura:component>